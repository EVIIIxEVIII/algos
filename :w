#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t; cin >> t;
    while (t--) {
        int n; cin >> n;

        vector<int> a(n);
        for (int i = 0; i < n; ++i) {
            cin >> a[i];
        }

        int total = 1;
        vector<int> count(n+1, 0);
        count[a[0]] = 1;

        int nextTotal = 0;
        vector<int> nextCount(n+1, 0);

        int ans = 1;
        for (int i = 1; i < n; ++i) {
            if (count[a[i]]) {
                total--;
                count[a[i]]--;
            }

            nextTotal++;
            nextCount[a[i]]++;

            if (!total) {
                ans++;
                total = nextTotal;
                count = nextCount;
                nextTotal = 0;
                nextCount.assign(n+1, 0);
            }
        }

        cout << ans << '\n';
    }
    return 0;
}
