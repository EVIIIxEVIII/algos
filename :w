#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t; cin >> t;
    while (t--) {
        int n; cin >> n;

        vector<vector<int>> queues(n, vector<int>(n));
        for (int i = 0; i < n; ++i) {
            for (int j = 0; j < n; ++j) {
                cin >> queues[i][j];
            }
        }

        vector<vector<int>> prefix(n, vector<int>(n+1));
        for (int i = 0; i < n; ++i) {
            int rowSum = 0;
            for (int j = 0; j < n; ++j) {
                rowSum += queues[i][j];
            }

            prefix[i][0] = rowSum;
            for (int j = 1; j <= n; ++j) {
                prefix[i][j] = prefix[i][j-1] - queues[i][j-1];
            }
        }

        for (int i = 0; i < n; ++i) {
            for (int j = 0; j < n; ++j) {

            }
        }

    }

    return 0;
}
