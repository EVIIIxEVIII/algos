#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t; cin >> t;
    while (t--) {
        int n, rk, ck, rd, cd;
        cin >> n >> rk >> ck >> rd >> cd;

        int maxMoves = max({n - cd, n - rd, rd, cd});

        if (rk > rd && ck > cd) { // top right


        } else if (rk > rd && ck < cd) { // top left

        }

        cout << "Max moves: " << maxMoves << '\n';
    }
    return 0;
}
